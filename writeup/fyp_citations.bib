journal = {Developmental neuropsychology},
number = {4},
pages = {447--473},
pmid = {18568899},
title = {{Development of neural networks for exact and approximate calculation: a FMRI study.}},
url = {http://www.tandfonline.com/doi/pdf/10.1080/87565640802101474 http://www.tandfonline.com/doi/abs/10.1080/87565640802101474},
