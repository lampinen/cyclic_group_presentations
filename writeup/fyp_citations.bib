issn = {0278-7393},
journal = {Journal of Experimental Psychology: Learning, Memory, and Cognition},
number = {5},
pages = {463--474},
title = {{Effects of goal specificity on means-ends analysis and learning.}},
url = {http://psycnet.apa.org/journals/xlm/8/5/463.pdf},
volume = {8},
year = {1982}
}
