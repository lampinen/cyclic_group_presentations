journal = {Cognitive Brain Research},
keywords = {Brain network,Calculation,Mental arithmetic,fMRI},
number = {3},
pages = {397--405},
pmid = {15722210},
title = {{The neural substrate of arithmetic operations and procedure complexity}},
url = {http://ac.els-cdn.com/S0926641004002514/1-s2.0-S0926641004002514-main.pdf?{\_}tid=1fbfd03a-873e-11e5-9a93-00000aacb362{\&}acdnat=1447113841{\_}b893914bf5d303f0a979ca76c7724b3b},
volume = {22},
